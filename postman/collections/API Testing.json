			"name": "Create Users",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "06e0ead0-3379-4659-96e5-260f4d871885",
						"exec": [
							"// Postman variables\r",
							"// pm.environment.get(\"variable_key\");\r",
							"// pm.globals.get(\"variable_key\");\r",
							"// pm.variables.get(\"variable_key\");\r",
							"// pm.collectionVariables.get(\"variable_key\");\r",
							"\r",
							"pm.test(\"E2E Test\", function(){\r",
							"console.log(pm.environment.get(\"production\"));\r",
							"\r",
							"    pm.test(\"Status code is 201\", function () {\r",
							"        pm.response.to.have.status(201);\r",
							"    });\r",
							"\r",
							"    pm.test(\"Test the environment variable (URI)\", function () {\r",
							"        pm.expect(pm.environment.get(\"baseURI\")).to.equal(\"https://reqres.in/api\");\r",
							"    });\r",
							"\r",
							"    pm.test(\"Test the global variable\", function(){\r",
							"        var jsonResponse = pm.response.json();\r",
							"        pm.globals.set(\"id\",jsonResponse.id)\r",
							"        pm.expect(pm.globals.get(\"id\")).to.equal(jsonResponse.id)\r",
							"    });\r",
							"\r",
							"    pm.test(\"Test the normal variable\", function(){\r",
							"        var jsonResponse = pm.response.json();\r",
							"        pm.variables.set(\"name\", jsonResponse.name);\r",
							"        pm.expect(pm.variables.get(\"name\")).to.equal(jsonResponse.name)\r",
							"    });\r",
							"\r",
							"    pm.test(\"Test the collectiion variable\", function(){\r",
							"        var jsonResponse = pm.response.json();\r",
							"        pm.collectionVariables.set(\"job\", jsonResponse.job);\r",
							"        pm.expect(pm.collectionVariables.get(\"job\")).to.equal(jsonResponse.job)\r",
							"    });\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"    \r",
							"});"
						],
						"type": "text/javascript"
					}
